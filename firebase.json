{
  "functions": {
    "source": "functions",
    "runtime": "nodejs20"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "cargo-go-b5f77",
          "appId": "1:1001281401892:android:b70989512cee13dfb0d4b1",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "cargo-go-b5f77",
          "configurations": {
            "android": "1:1001281401892:android:b70989512cee13dfb0d4b1",
            "ios": "1:1001281401892:ios:8ca7b7ce2b5f0c20b0d4b1",
            "macos": "1:1001281401892:ios:8ca7b7ce2b5f0c20b0d4b1",
            "web": "1:1001281401892:web:f1176dd499e5bec4b0d4b1",
            "windows": "1:1001281401892:web:8882614dc275d754b0d4b1"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "index.html",
        "headers": [{"key": "Cache-Control", "value": "no-cache, no-store, must-revalidate"}]
      },
      {
        "source": "flutter_service_worker.js",
        "headers": [{"key": "Cache-Control", "value": "no-cache, no-store, must-revalidate"}]
      },
      {
        "source": "main.dart.js",
        "headers": [{"key": "Cache-Control", "value": "no-cache, no-store, must-revalidate"}]
      },
      {
        "source": "sharazan.html",
        "headers": [{"key": "Cache-Control", "value": "no-cache, no-store, must-revalidate"}]
      },
      {
        "source": "crudo.html",
        "headers": [{"key": "Cache-Control", "value": "no-cache, no-store, must-revalidate"}]
      },
      {
        "source": "pilingas.html",
        "headers": [{"key": "Cache-Control", "value": "no-cache, no-store, must-revalidate"}]
      },
      {
        "source": "farmacias_madrid.html",
        "headers": [{"key": "Cache-Control", "value": "no-cache, no-store, must-revalidate"}]
      }
    ],
    "rewrites": [
      {
        "source": "sharazan.html",
        "destination": "/sharazan.html"
      },
      {
        "source": "crudo.html",
        "destination": "/crudo.html"
      },
      {
        "source": "pilingas.html",
        "destination": "/pilingas.html"
      },
      {
        "source": "farmacias_madrid.html",
        "destination": "/farmacias_madrid.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
